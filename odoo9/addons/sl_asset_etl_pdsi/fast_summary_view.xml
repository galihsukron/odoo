<?xml version="1.0"?>
<openerp>
	<data>

		<record model="ir.ui.view" id="view_fast_summary_tree">
			<field name="name">fast.summary.tree2</field>
			<field name="model">fast.summary</field>
			<field name="type">tree</field>
			<field name="field_parent">child_ids</field> 
			<field name="arch" type="xml">
				<tree string="Fast Download Summary" colors="red:failed and (failed&gt;0);">
					<field icon="icon" name="name"/>
					<field name="company_id"/>
                    <field name="downloaded" />					
					<field name="found" />
					<field name="inserted" />
					<field name="failed" />
										
<!--                    <field name="started" /> --> 
<!--                    <field name="finished" /> -->					
					<field name="duration" />
					<field name="note" />
				</tree>
			</field>
		</record>
		
		<record model="ir.actions.act_window" id="action_fast_download_summary_form">
			<field name="name">Fast Download Summary</field>
			<field name="type">ir.actions.act_window</field>
			<field name="res_model">fast.summary</field>
			<field name="domain">[('parent_id','=',False)]</field>
			<field name="view_type">tree</field>
			<field name="view_id" ref="view_fast_summary_tree"/>
			<field name="auto_refresh">0</field>
		</record>
		
		<menuitem parent="pn2.menu_sc_maintenance" name="Fast Download Summary" id="menu_fast_download_summary_tree" 
			action="action_fast_download_summary_form"/>	
							
		<wizard
            string="Import Planning dari MS2 View (Fast)"
            model="fast.summary"
            name="pn.import.planning2.MS2.sqlpure"
            id="wizard_import_planning2_MS2_sqlpure_fs"/>
             
        <wizard
            string="Import Data LO (Fast)"
            model="fast.summary"
            name="pn.import.lo.fast"
            id="wizard_import_lo_fast_fs"/>  
	</data>
</openerp>		